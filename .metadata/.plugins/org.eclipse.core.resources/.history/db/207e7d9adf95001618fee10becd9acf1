package hr.fer.zemris.optjava.dz2;

public class Sustav {

	public static void main(String[] args) {
		
		if(args.length != 3) {
			System.err.println("Program ocekuje 3 argumenta");
		}
		
		NumOptAlgorithms.algoritamGradijentnogSpusta(new Funkcija3(new CitacSustavaLinJedn(args[2])), 100, null);
		
	}
	
}
