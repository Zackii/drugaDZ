package hr.fer.zemris.optjava.dz2;

public class Main {

	public static void main(String args[]) {

		double[] ds = new double[] { -10.7418660695, -1.1423058406, 13.0700480447, 10.6666666667, -3.8742827879,
				2.4430637876, 4.6666666667, -1.3333333334, -12.720127499, -5.9249167014 };

		double[] ks = new double[] { -3.000, 0.000, 3.000, 0.000, -3.000, 2.000, 0.000, -3.000, -1.000, -2.000 };

		double sum = 0;
		for (int i = 0; i < 10; i++) {
			sum += ds[i] * ks[i];
		}

		System.out.println(sum);

	}

}
